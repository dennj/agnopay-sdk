{
  "name": "@agnopay/sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for AgnoPay payment processing",
  "license": "MIT",
  "author": "AgnoPay",
  "repository": {
    "type": "git",
    "url": "https://github.com/dennj/agnopay-sdk.git"
  },
  "bugs": "https://github.com/dennj/agnopay-sdk/issues",
  "homepage": "https://github.com/dennj/agnopay-sdk#readme",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js",
      "require": "./dist/client.cjs"
    },
    "./hooks": {
      "types": "./dist/client-hooks.d.ts",
      "import": "./dist/client-hooks.js",
      "require": "./dist/client-hooks.cjs"
    },
    "./components": {
      "types": "./dist/components/AgnoPayCheckout.d.ts",
      "import": "./dist/components/AgnoPayCheckout.js",
      "require": "./dist/components/AgnoPayCheckout.cjs"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepare": "npm run build",
    "prepublishOnly": "npm run clean && npm run build && npm run type-check"
  },
  "dependencies": {},
  "peerDependencies": {
    "next": ">=14.0.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "next": "^16",
    "react": "^19",
    "react-dom": "^19",
    "tsup": "^8.0.0",
    "typescript": "^5"
  },
  "keywords": [
    "agnopay",
    "payment",
    "checkout",
    "sdk",
    "typescript",
    "react",
    "payments",
    "wallet"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}